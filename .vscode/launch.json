{
    "version": "0.1.0",
    "configurations": [
        {
            "name": "Attach to Chrome with sourcemaps",
            "type": "chrome",
            "request": "attach",
            "port": 9222,
            "sourceMaps": true
        },
        {
            "name": "Attach to Chrome from file index.html",
            "type": "chrome",
            "request": "attach",
            "port": 9222,
            "webRoot": "${workspaceRoot}/index.html"
        }
    ]
}